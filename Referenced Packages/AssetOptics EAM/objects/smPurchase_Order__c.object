    <validationRules>
        <fullName>New_Record_Entry_Status</fullName>
        <active>true</active>
        <description>Controls the valid statuses for entering a new Purchase Order</description>
        <errorConditionFormula>ISNEW() &amp;&amp; NOT(( ISPICKVAL( Status__c , &quot;Request&quot;) || ISPICKVAL( Status__c , &quot;Waiting Approval&quot;) || ISPICKVAL( Status__c , &quot;Approved&quot;)))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>The initial status for a Purchase Order must be set to Request, Waiting Approval, or Approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Open_Part_Qty_Status_Change</fullName>
        <active>true</active>
        <description>Ensures that the Purchase Order Status cannot be updated to Canceled, Completed, Closed if the Open Qty on any PO Part line &gt; 0</description>
        <errorConditionFormula>(ISPICKVAL( Status__c , &quot;Canceled&quot;) || ISPICKVAL( Status__c , &quot;Completed&quot;) || ISPICKVAL( Status__c , &quot;Closed&quot;)) &amp;&amp;  Parts_Open_Qty__c &gt;0</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>The Purchase Order Status cannot be updated to Canceled, Completed, or Closed if there is an Open Qty on any PO Part line.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PO_VST_to_Approved</fullName>
        <active>true</active>
        <description>Controls the Valid PO Status Transition to Approved</description>
        <errorConditionFormula>ISPICKVAL( Status__c , &quot;Approved&quot;) &amp;&amp; NOT(TEXT(PRIORVALUE( Status__c )) = &quot;Waiting Approval&quot;) &amp;&amp; NOT(ISNEW()) &amp;&amp;  ISCHANGED(Status__c)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>A Purchase Order Status can be updated to Approved only from a previous Status of Waiting Approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PO_VST_to_Canceled</fullName>
        <active>true</active>
        <description>Controls the Valid PO Status Transition to Canceled</description>
        <errorConditionFormula>ISPICKVAL( Status__c , &quot;Canceled&quot;) &amp;&amp; NOT((TEXT(PRIORVALUE( Status__c )) = &quot;In Progress&quot; || TEXT(PRIORVALUE( Status__c )) = &quot;Released&quot; || TEXT(PRIORVALUE( Status__c )) = &quot;Ready to Release&quot;)) &amp;&amp; NOT(ISNEW()) &amp;&amp; ISCHANGED(Status__c)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>A Purchase Order Status can be updated to Canceled only from a previous Status of Ready to Release, Released, or In Progress</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PO_VST_to_Closed</fullName>
        <active>true</active>
        <description>Controls the Valid PO Status Transition to Closed</description>
        <errorConditionFormula>ISPICKVAL( Status__c , &quot;Closed&quot;) &amp;&amp; NOT(TEXT(PRIORVALUE( Status__c )) = &quot;Completed&quot;) &amp;&amp; NOT(ISNEW()) &amp;&amp; ISCHANGED(Status__c)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>A Purchase Order Status can be updated to Closed only from a previous Status of Completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PO_VST_to_Completed</fullName>
        <active>true</active>
        <description>Controls the Valid PO Status Transition to Completed</description>
        <errorConditionFormula>ISPICKVAL( Status__c , &quot;Completed&quot;) &amp;&amp; NOT((TEXT(PRIORVALUE( Status__c )) = &quot;In Progress&quot; || TEXT(PRIORVALUE( Status__c )) = &quot;Released&quot;)) &amp;&amp; NOT(ISNEW()) &amp;&amp; ISCHANGED(Status__c)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>A Purchase Order Status can be updated to Completed only from a previous Status of Released or In Progress</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PO_VST_to_In_Progress</fullName>
        <active>true</active>
        <description>Controls the Valid PO Status Transition to In Progress</description>
        <errorConditionFormula>ISPICKVAL( Status__c , &quot;In Progress&quot;) &amp;&amp; NOT(TEXT(PRIORVALUE( Status__c )) = &quot;Released&quot;) &amp;&amp; NOT(ISNEW()) &amp;&amp; ISCHANGED(Status__c)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>A Purchase Order Status can be updated to In Progress only from a previous Status of Released</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PO_VST_to_Ready_to_Release</fullName>
        <active>true</active>
        <description>Controls the Valid PO Status Transition to Ready to Release</description>
        <errorConditionFormula>ISPICKVAL( Status__c , &quot;Ready to Release&quot;) &amp;&amp; NOT(TEXT(PRIORVALUE( Status__c )) = &quot;Approved&quot;) &amp;&amp; NOT(ISNEW()) &amp;&amp; ISCHANGED(Status__c)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>A Purchase Order Status can be updated to Ready to Release only from a previous Status of Approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PO_VST_to_Rejected</fullName>
        <active>true</active>
        <description>Controls the Valid PO Status Transition to Rejected</description>
        <errorConditionFormula>ISPICKVAL( Status__c , &quot;Rejected&quot;) &amp;&amp; NOT((TEXT(PRIORVALUE( Status__c )) = &quot;Approved&quot; || TEXT(PRIORVALUE( Status__c )) = &quot;Waiting Approval&quot;)) &amp;&amp; NOT(ISNEW()) &amp;&amp; ISCHANGED(Status__c)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>A Purchase Order Status can be updated to Rejected only from a previous Status of Approved or Waiting Approval</errorMessage>
    </validationRules>
    <validationRules>
