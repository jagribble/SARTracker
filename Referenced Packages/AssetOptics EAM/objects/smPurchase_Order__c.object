        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>A Purchase Order Status can be updated to Released only from a previous Status of Approved or Ready to Release</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PO_VST_to_Waiting_Approval</fullName>
        <active>true</active>
        <description>Controls the Valid PO Status Transition to Waiting Approval</description>
        <errorConditionFormula>ISPICKVAL( Status__c , &quot;Waiting Approval&quot;) &amp;&amp; NOT((TEXT(PRIORVALUE( Status__c )) = &quot;Request&quot; || TEXT(PRIORVALUE( Status__c )) = &quot;Rejected&quot;)) &amp;&amp; NOT(ISNEW()) &amp;&amp; ISCHANGED(Status__c)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>A Purchase Order Status can be updated to Waiting Approval only from a previous Status of Request or Rejected</errorMessage>
    </validationRules>
