    <validationRules>
        <fullName>Min_Qty_NonNegative</fullName>
        <active>false</active>
        <description>Test to ensure that the Min Qty is a non-negative value (Obsolete - Field Deprecated)</description>
        <errorConditionFormula>Min_Qty__c   &lt;  0</errorConditionFormula>
        <errorDisplayField>Min_Qty__c</errorDisplayField>
        <errorMessage>Min Qty cannot be a negative value (Obsolete)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Purchase_Pack_Qty_Update</fullName>
        <active>true</active>
        <description>Ensures that the Purchase Pack Qty cannot be Updated when the On Order Qty is Greater than Zero</description>
        <errorConditionFormula>ISCHANGED( Reorder_Qty__c )  &amp;&amp;  On_Order_Qty__c  &gt; 0</errorConditionFormula>
        <errorDisplayField>Reorder_Qty__c</errorDisplayField>
        <errorMessage>The Purchase Pack Qty cannot be changed while the On Order Qty is Greater than Zero.  Please receive or cancel open purchase order line items.  The preferred approach is to create a new Part record with an updated Purchase Pack Qty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Purchase_Price_GE_Zero</fullName>
        <active>true</active>
        <description>Ensures that the Purchase Price is Greater Than or Equal to Zero and Is Not Null</description>
        <errorConditionFormula>Purchase_Price__c  &lt; 0 ||  ISNULL( Purchase_Price__c )</errorConditionFormula>
        <errorDisplayField>Purchase_Price__c</errorDisplayField>
        <errorMessage>The Purchase Price cannot be a Negative or a Null Value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ROQ_LE_One</fullName>
        <active>true</active>
        <description>The &quot;Purchase Pack Qty&quot; must be Greater Than or Equal to One (1)</description>
        <errorConditionFormula>Reorder_Qty__c  &lt;= 0 ||  ISBLANK(Reorder_Qty__c)</errorConditionFormula>
        <errorDisplayField>Reorder_Qty__c</errorDisplayField>
        <errorMessage>The Purchase Pack Qty must be Greater Than or Equal to one (1)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reorder_Point_NonNegative</fullName>
        <active>true</active>
        <description>Ensures that the Min Qty is a Non-negative value.  The field labeled &quot;Min Qty&quot; is the Reorder Point Field</description>
        <errorConditionFormula>Reorder_Point__c  &lt; 0</errorConditionFormula>
        <errorDisplayField>Reorder_Point__c</errorDisplayField>
        <errorMessage>The Min Qty must be Greater Than or Equal to zero (0)</errorMessage>
